               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="357.0" prefHeight="35.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="100.0" text="Date:" />
                  <Label fx:id="dateField" prefWidth="200.0" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="457.0" prefHeight="35.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="100.0" text="Start:" />
                  <Label fx:id="startField" prefWidth="200.0" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="397.0" prefHeight="35.0" prefWidth="200.0">
               <children>
                  <Label layoutX="105.0" layoutY="19.0" prefWidth="100.0" text="End:">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="endField" prefWidth="200.0" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#editButton" prefWidth="80.0" text="Edit" />
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelButton" prefWidth="80.0" text="Cancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
