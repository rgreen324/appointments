               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="157.0" prefHeight="35.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="100.0" text="Contact:" />
                  <Label fx:id="contactField" prefWidth="200.0" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="357.0" prefHeight="35.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="100.0" text="Type:" />
                  <Label fx:id="typeField" prefWidth="200.0" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="100.0" text="URL:" />
