               <VBox.margin>
                  <Insets bottom="20.0" top="10.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" layoutX="20.0" layoutY="87.0" prefHeight="35.0" prefWidth="200.0">
